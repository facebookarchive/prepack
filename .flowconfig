[ignore]
.*/node_modules/babel.*
.*/test262/.*
.*/node_modules/.*\.json
.*/lib/.*
.*/test/serializer/.*
.*/scripts/prettier.js

[include]

[libs]
flow-libs

[options]
suppress_comment= \\(.\\|\n\\)*\\$FlowFixMe
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
module.name_mapper='/lib/' -> '/src/'
